<h1>Authors</h1>
<%= link_to 'Log out', destroy_user_session_path, method: :delete %>
<%= link_to 'Create a book', new_book_path %>
<%= link_to 'Profile', user_path(current_user) %>
<hr>
<% @users.each do |user| %>
  <p>Name: <%= user.name %></p>
  <p>Age: <%= user.age %></p>
  <%= link_to 'Show', user_path(user) %>
  <hr>
<% end %>